{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "webpack",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/webpack/bin/webpack.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "webpack-dev-server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/webpack-dev-server/bin/webpack-dev-server.js"
        }
    ]
}